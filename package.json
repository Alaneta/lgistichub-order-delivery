{
  "name": "aper-lh-order-delivery",
  "version": "1.5.2",
  "scripts": {
    "start": "yarn run build && yarn run serve:local",
    "debug": "yarn run serve:debug",
    "build": "yarn run clean && yarn run compile",
    "compile": "tsc",
    "clean": "rm -rf dist",
    "test": "jest",
    "coverage": "jest --coverage",
    "serve:production": "node newrelic ./dist/app.js",
    "serve:local": "npx nodemon -L --watch './**/*.ts' --exec NODE_OPTIONS='newrelic ts-node --files' -r dotenv/config src/app.ts",
    "serve:debug": "npx nodemon -L --watch './**/*.ts' --exec node --inspect=9222 -r ts-node/register -r dotenv/config newrelic src/app.ts",
    "lint": "eslint --ext .ts src/**/*.ts & tsc --pretty",
    "format": "eslint --ext .ts --fix src/**/*.ts & tsc --pretty & prettier --write 'src/**/*.ts'",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.1.0",
    "@types/body-parser": "^1.19.2",
    "@types/chai": "^4.3.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.191",
    "@types/luxon": "^2.3.1",
    "@types/mocha": "^9.1.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^16.11.1",
    "@types/pino-http": "^5.8.1",
    "@types/supertest": "^2.0.12",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/validator": "^13.7.2",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "eslint": "^8.32.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^28.1.0",
    "nodemon": "^2.0.14",
    "prettier": "^2.8.3",
    "sequelize-mock": "^0.10.2",
    "ts-jest": "^28.0.0-next.3",
    "ts-node": "^10.8.1",
    "tsc": "^2.0.4",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "aws-sdk": "^2.1227.0",
    "axios": "^0.27.2",
    "binary-uuid": "^2.0.3",
    "body-parser": "^1.20.0",
    "chai": "^4.3.6",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.0",
    "envalid": "^7.3.1",
    "express": "^4.17.1",
    "http-status": "^1.5.1",
    "jest-junit": "^13.2.0",
    "joi": "^17.6.0",
    "lodash": "^4.17.21",
    "luxon": "^2.3.2",
    "mocha": "^10.0.0",
    "mysql2": "^2.3.3",
    "newrelic": "latest",
    "pino": "^7.11.0",
    "pino-http": "^7.0.0",
    "pino-pretty": "^7.6.1",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.19.0",
    "sequelize-typescript": "^2.1.3",
    "sqlite3": "^5.0.6",
    "supertest": "^6.2.3",
    "swagger-jsdoc": "^6.2.1",
    "swagger-ui-express": "^4.4.0"
  },
  "engines": {
    "node": ">=16"
  }
}
